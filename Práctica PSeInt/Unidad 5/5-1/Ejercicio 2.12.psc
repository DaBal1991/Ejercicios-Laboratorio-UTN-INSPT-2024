Algoritmo sin_titulo
	
	// Se leen dos enteros N y M, y se emite la tabla de multiplicar de N entre 1 y M
	
	Definir N, M, i Como Entero;
	
	Imprimir "Ingresar un valor N: ";
	Leer N;
	
	Imprimir "Ingresar valor M: ";
	Leer M;
	
	Para i = 1 hasta M Hacer
		Imprimir i, " x ", N, " = ", i*N;
	FinPara
	
FinAlgoritmo
